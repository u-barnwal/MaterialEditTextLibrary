<?xml version="1.0" encoding="utf-8"?>
<resources>

  <declare-styleable name="MaterialEditText">
    <!-- TIL -->
    <attr name="met_errorContentDescription" format="string" />
    <attr name="met_boxBackgroundColor" format="color" />
    <attr name="met_boxBackgroundMode">
      <enum name="none" value="0" />
      <enum name="filled" value="1" />
      <enum name="outline" value="2" />
    </attr>
    <attr name="met_errorEnabled" format="boolean" />
    <attr name="met_enabled" format="boolean" />
    <attr name="met_required" format="boolean" />

    <!-- TIET -->
    <attr name="met_hint" format="string" />
    <attr name="met_inputType" format="string" />
    <attr name="met_maxLines" format="integer" min="0" />
  </declare-styleable>

</resources>